{
  "name": "sevenpenguin",
  "version": "1.6.2",
  "description": "An electron-vue project",
  "scripts": {
    "build": "npm run pack && build",
    "build:dir": "npm run pack && build --dir",
    "dev": "node tasks/runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack  --progress --colors --config webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack  --progress --colors --config webpack.renderer.config.js"
  },
  "build": {
    "productName": "七只企鹅App",
    "appId": "org.simulatedgreg.electron-vue",
    "category": "public.app-category.-tools",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "mac": {
      "icon": "app/icons/icon.icns"
    },
    "win": {
      "icon": "app/icons/icon3.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  },
  "author": "B-soft",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.6.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "^1.7.5",
    "electron-builder": "^11.4.4",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-rebuild": "^1.1.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.16.1",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "md5": "^2.2.1",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "node-sass": "^4.5.2",
    "require-dir": "^0.3.0",
    "sass-loader": "6.0.5",
    "style-loader": "^0.13.1",
    "tree-kill": "^1.1.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^2.0.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^10.0.2",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-runtime": "6.23.0",
    "echarts": "^3.3.2",
    "element-ui": "1.3.4",
    "getmac": "^5.1.0",
    "http-proxy": "^1.16.2",
    "normalize.css": "3.0.2",
    "nprogress": "^0.2.0",
    "vue": "^2.4.2",
    "vue-electron": "^1.0.6",
    "vue-router": "2.5.3",
    "vuex": "2.3.1"
  }
}
